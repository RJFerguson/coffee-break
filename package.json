    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.0.4",
    "eslint-config-prettier": "^9.1.0",
    "jest-environment-jsdom": "^29.7.0",
    "lucide-react": "^0.378.0",
    "next": "14.2.3",
    "next-themes": "^0.3.0",
    "prisma": "^5.13.0",
    "react": "^18",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18",
