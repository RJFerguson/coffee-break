{
  "name": "coffee-break",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "seed": "bun ./seed.js",
    "bootstrap": "npx prisma db push && npx prisma generate",
    "dev": "bun run bootstrap && bun next dev --port 3000",
    "demo": "bun run bootstrap && bun run seed && bun next dev --port 3000",
    "build": "prisma generate && bun next build",
    "start": "prisma migrate deploy &&  bun next start",
    "lint": "bun next lint",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "migrate": "prisma migrate deploy",
    "start:migrate:dev": "prisma migrate deploy && bun next dev",
    "format": "npx prettier --write \"**/*.{ts,tsx,md,mdx,css,scss}\""
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
